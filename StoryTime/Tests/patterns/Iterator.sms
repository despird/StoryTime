{
	CNodeCollection c.();
	CNode node1.("first");
	
	c.AddNode(node[node1])
	{
		<%
		_list.Add(node); 
		 nodeMax++;	
		%>
	};
	
	CNode node2.("second");
	c.AddNode(node2);				//need not re-define
	
	CNode node3.("third");	//need not re-define
